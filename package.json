    "autoprefixer": "^10.4.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "lint-staged": "^12.4.1",
