{
  "name": "guebbit-vue-library",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/vuelib.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "scripts": {
